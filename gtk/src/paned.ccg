#include <gtk/gtk.h>

namespace Gtk
{
void Paned::unset_start_child()
{
  gtk_paned_set_start_child(gobj(), nullptr);
}

void Paned::unset_end_child()
{
  gtk_paned_set_end_child(gobj(), nullptr);
}
} // namespace Gtk
