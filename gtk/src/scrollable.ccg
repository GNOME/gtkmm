#include <gtk/gtk.h>

namespace Gtk
{

void Scrollable::unset_hadjustment()
{
  gtk_scrollable_set_hadjustment(gobj(), nullptr);
}

void Scrollable::unset_vadjustment()
{
  gtk_scrollable_set_vadjustment(gobj(), nullptr);
}

} //namespace Gtk
